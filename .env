# API Keys for MockMate Desktop App
DEEPGRAM_API_KEY=8178bb79a552d96705f111b6e460e11198d2fa41
OPENAI_API_KEY=

# Deepgram Configuration - All configurable options
# Model selection - Options: nova-2, nova, whisper, enhanced, base
DEEPGRAM_MODEL=nova-2

# Language code - Examples: en-US, en-GB, es, fr, de, ja, etc.
DEEPGRAM_LANGUAGE=en-US

# Smart formatting - Automatically formats transcripts (true/false)
DEEPGRAM_SMART_FORMAT=true

# Interim results - Get partial transcripts before final result (true/false)
DEEPGRAM_INTERIM_RESULTS=true

# Endpointing - Milliseconds of silence before finalizing transcript (50-2000)
DEEPGRAM_ENDPOINTING=300

# Keep connection alive (true/false)
DEEPGRAM_KEEP_ALIVE=true

# Punctuation - Add punctuation to transcripts (true/false)
DEEPGRAM_PUNCTUATE=true

# Profanity filter - Filter out profanity (true/false)
DEEPGRAM_PROFANITY_FILTER=false

# Redact sensitive information - Comma-separated list (pci, numbers, ssn, etc.)
# Example: DEEPGRAM_REDACT=pci,ssn,numbers
DEEPGRAM_REDACT=

# Speaker diarization - Identify different speakers (true/false)
DEEPGRAM_DIARIZE=false

# Multichannel processing - Process each channel separately (true/false)
DEEPGRAM_MULTICHANNEL=false

# Alternative transcripts - Number of alternative transcripts (1-10)
DEEPGRAM_ALTERNATIVES=1

# Convert numbers to numerals (true/false)
DEEPGRAM_NUMERALS=false

# Search terms - Comma-separated list of terms to boost recognition
# Example: DEEPGRAM_SEARCH=interview,technical,algorithm
DEEPGRAM_SEARCH=

# Replace terms - Comma-separated list of find:replace pairs
# Example: DEEPGRAM_REPLACE=um:,uh:,like:
DEEPGRAM_REPLACE=

# Keywords - Comma-separated list of important keywords to boost
# Example: DEEPGRAM_KEYWORDS=MockMate,interview,coding,algorithm
DEEPGRAM_KEYWORDS=

# Keyword boost - How to boost keywords (legacy, latest)
DEEPGRAM_KEYWORD_BOOST=legacy

# Other environment variables can be added here as needed
